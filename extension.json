{
	"name": "InterwikiDispatcher",
	"version": "1.3.0",

	"author": [
		"MarkusRost",
		"alex4401"
	],
	"url": "https://mediawiki.org/wiki/Extension:InterwikiDispatcher",
	"descriptionmsg": "interwikidispatcher-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",

	"requires": {
		"MediaWiki": ">= 1.35.0",
		"platform": {
			"php": ">= 7.4"
		}
	},

	"MessagesDirs": {
		"InterwikiDispatcher": [
			"i18n"
		]
	},

	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\InterwikiDispatcher\\": "includes/"
	},
	
	"HookHandlers": {
		"Main": {
			"class": "MediaWiki\\Extension\\InterwikiDispatcher\\InterwikiDispatcherHooks",
			"services": [
				"MainConfig"
			]
		}
	},
	"Hooks": {
		"GetLocalURL": "Main"
	},

	"config_prefix": "wgIWD",
	"config": {
		"Prefixes": {
			"value": []
		}
	},

	"manifest_version": 2
}
